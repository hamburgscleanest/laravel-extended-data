{
  "name": "hamburgscleanest/laravel-extended-data",
  "description": "Handle additional data for your models without touching your existing tables",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andr√© Biel",
      "email": "biermitl@gmail.com",
      "homepage": "https://www.andrebiel.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.6",
    "illuminate/database": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "orchestra/testbench": "^3.6",
  },
  "autoload": {
    "psr-4": {
      "hamburgscleanest\\LaravelExtendedData\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hamburgscleanest\\LaravelExtendedData\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "hamburgscleanest\\LaravelExtendedData\\LaravelExtendedDataServiceProvider"
      ]
    }
  }
}
